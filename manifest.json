{
  "manifest_version": 3,
  "name": "Saxo to Tradezella Uploader",
  "version": "0.1.1",
  "description": "サクソバンク証券の取引履歴をTradezella形式のCSVに変換するChrome拡張機能です。",
  "content_scripts": [
    {
      "matches": [
        "https://app.tradezella.com/*"
      ],
      "js": ["dist/content.js"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": [
    "https://app.tradezella.com/*"
  ]
}
